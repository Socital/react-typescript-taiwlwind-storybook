{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates": {
        "lazy": "config/templates/TemplateName.lazy.tsx",
        "style": "config/templates/TemplateName.module.scss",
        "story": "config/templates/TemplateName.stories.tsx",
        "test": "config/templates/TemplateName.test.tsx",
        "component": "config/templates/TemplateName.tsx"
      }
    }
  }
}
