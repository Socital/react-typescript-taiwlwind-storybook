{
  "name": "socital-webapp-client-v2",
  "version": "1.0.0",
  "main": "index.js",
  "license": "BSD-2-Clause",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "generate:component": "npx generate-react-cli component",
    "lint": "eslint src --ext .js,.jsx && yarn prettify",
    "dev": "webpack serve --config ./config/webpack.dev.js ",
    "build": "webpack --config ./config/webpack.dev.js --profile --json > stats.json",
    "build:production": "NODE_ENV=production webpack --config ./config/webpack.prod.js --profile --json > stats.json",
    "prettify": "yarn prettier \"**/*.*(js|jsx)\" --ignore-path=.prettierignore --write",
    "lint-fix": "eslint src --ext .js,.jsx --fix && yarn prettify",
    "test": "mocha test/** --extension js,ts -r ts-node/register -r jsdom-global -r mock-local-storage --recursive --timeout 5000 --exit -b",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged --verbose"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^2.1.4",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/builder-webpack5": "^6.2.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.2.9",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/react-dom": "^17.0.6",

    "autoprefixer": "^10.2.5",

    "eslint": "^7.26.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",

    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",

    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.15",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^5.3.0",

    "storybook-css-modules-preset": "^1.1.1",

    "sass": "^1.34.1",
    "sass-loader": "^12.0.0",
    "style-loader": "^2.0.0",
    "css-loader": "^5.2.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",

    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "terser-webpack-plugin": "^5.1.2",
    "html-webpack-plugin": "^5.3.1",
    "dotenv-webpack": "^6.0.4",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
